        <Label x:Name="label" Content="信道数量" HorizontalAlignment="Left" Margin="51,28,0,0" Width="69" FontSize="14" Foreground="#FF0023FF" RenderTransformOrigin="1.638,0.793" Height="35" VerticalAlignment="Top"/>

    </Grid>
</Window>
