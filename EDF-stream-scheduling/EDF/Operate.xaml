        <RadioButton Content="非抢占" Height="16" HorizontalAlignment="Left" Margin="68,93,0,0" x:Name="radioButton3" VerticalAlignment="Top" IsChecked="True" Checked="radioButton3_Checked" />
        <RadioButton Content="抢占" Height="16" HorizontalAlignment="Left" Margin="128,93,0,0" x:Name="radioButton4" VerticalAlignment="Top" Checked="radioButton4_Checked" />
        <RadioButton Content="RM" GroupName="method" Height="16" HorizontalAlignment="Left" Margin="146,68,0,0" x:Name="radioButton5" VerticalAlignment="Top" Checked="radioButton5_Checked" />
        <ComboBox x:Name="CPUCount" HorizontalAlignment="Left" Margin="128,32,0,0" VerticalAlignment="Top" Width="73" SelectedIndex="0" SelectionChanged="CPUCount_SelectionChanged">
