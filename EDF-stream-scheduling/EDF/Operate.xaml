        <Button x:Name="buttonForRun" Content="Schedule" HorizontalAlignment="Left" Margin="57,157,0,0" VerticalAlignment="Top" Width="130" Click="button_Click_1" IsEnabled="False"/>
        <Button x:Name="reSetButton" Content="reSet" HorizontalAlignment="Left" Margin="57,196,0,0" VerticalAlignment="Top" Width="130" Click="reSetButton_Click"/>
